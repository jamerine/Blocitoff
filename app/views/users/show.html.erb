<h1 class='list-title'><%= "#{@user.username}'s To-Do list."%></h1>
<div class='row'>
  <div class='col-md-8'>
    <h3>Blocitoff</h3>
  </div>
  <div class='col-md-6'>
    <%= render partial: 'items/form', locals: {user: @user} %>
  </div>
  <div id="items" class='items list-group col-md-8 pull-left'>
    <%= render @items %>
  </div>
</div>
